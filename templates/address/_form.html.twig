{{ form_start(form) }}
    {{ form_row(form._token) }}

    <div class="form-group">
        {{ form_row(form.batch) }}
    </div>

    <div class="form-group">
        {{ form_row(form.city) }}
    </div>

    <div class="form-group">
        {{ form_row(form.postalCode) }}
    </div>

    <div class="d-flex justify-content-end mt-4">
        {{ form_row(form.reset) }}
        {{ form_row(form.submit) }}
    </div>
{{ form_end(form, {'render_rest': false}) }}
